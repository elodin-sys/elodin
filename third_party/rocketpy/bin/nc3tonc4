#!/home/kush-mahajan/.venv/bin/python3
# -*- coding: utf-8 -*-
import sys
from netCDF4.utils import nc3tonc4
if __name__ == "__main__":
    if sys.argv[0].endswith("-script.pyw"):
        sys.argv[0] = sys.argv[0][:-11]
    elif sys.argv[0].endswith(".exe"):
        sys.argv[0] = sys.argv[0][:-4]
    sys.exit(nc3tonc4())
